#include<stdio.h>
#include<math.h>
int main()
{
   float x,y;
   int count=1;
   printf("Enter a number:");
   scanf("%f",&x);
   read:
   if(x<0)
   printf("Value is negative");
   else
   {
      {
      y=sqrt(x);
      printf("Square root of %f is %f\n",x,y);
      x=x+1;
      }
   count=count+ 1;
   if(count<=5)
   goto read;
   }
}
